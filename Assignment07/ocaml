let quicksort lst = List.sort compare lst

let rec mergesort lst =
  let merge l1 l2 =
    let rec aux acc l1 l2 = match l1, l2 with
      | [], l | l, [] -> List.rev_append acc l
      | h1::t1, h2::t2 ->
        if h1 <= h2 then aux (h1::acc) t1 l2
        else aux (h2::acc) l1 t2
    in aux [] l1 l2
  in
  match lst with
  | [] | [_] -> lst
  | _ ->
    let mid = List.length lst / 2 in
    let left = List.sub lst 0 mid in
    let right = List.sub lst mid (List.length lst - mid) in
    merge (mergesort left) (mergesort right)

let rec bubblesort lst =
  let rec pass lst =
    match lst with
    | [] | [_] -> lst
    | x::y::t ->
      if x > y then y :: pass (x::t)
      else x :: pass (y::t)
  in
  let rec loop lst n =
    if n = 0 then lst
    else loop (pass lst) (n - 1)
  in
  loop lst (List.length lst)

(* Higher-order function *)
let sort_with strategy lst = strategy lst

(* Example usage *)
let data = [5;2;9;1;5;6]

let () =
  let sorted = sort_with quicksort data in
  List.iter (Printf.printf "%d ") sorted;
  print_newline ()

